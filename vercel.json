{
  "functions": {
    "api/fetchData.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/((?!assets/).*)",
      "destination": "/api/ssr.js"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)\\.js$",
      "status": 404
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ]
}
