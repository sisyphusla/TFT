{
  "functions": {
    "api/fetchData.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/((?!assets/).*)",
      "destination": "/api/ssr.js"
    }
  ],
  "redirects": [
    { "source": "/api", "destination": "/api/(.*)\\.js$", "statusCode": 404 }
  ]
}
